{
    "require": {
        "speich/websitetemplate": "1.*",
        "tsmgeek/exif-tool_php_stayopen": "dev-master",
        "philharvey/exiftool": "11.*",
        "vanderlee/syllable": "^1.5",
        "PHP": ">7.",
        "ext-dom": "*",
        "ext-gd": "*",
        "ext-PDO": "*",
        "ext-exif": "*",
        "ext-sqlite3": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-enchant": "*",
        "ext-intl": "*"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "PhotoDatabase\\": "../scripts/php/class",
            "WebsiteTemplate\\": "vendor/speich/websitetemplate/scripts/php"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/speich/WebsiteTemplate"
        },
        {
            "type": "vcs",
            "url": "https://github.com/mkardakov/ExifTool_PHP_Stayopen"
        },
        {
            "type": "package",
            "package": {
                "name": "philharvey/exiftool",
                "version": "11.23",
                "dist": {
                    "url": "https://sno.phy.queensu.ca/~phil/exiftool/Image-ExifTool-11.23.tar.gz",
                    "type": "tar"
                }
            }
        }
    ],
    "name": "speich/photodb",
    "description": "Database to manage photos",
    "license": ["CC-BY-NC-SA-4.0"]
}
