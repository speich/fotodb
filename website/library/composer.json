{
  "require": {
    "speich/websitetemplate": "0.*",
    "tsmgeek/exif-tool_php_stayopen": "dev-master",
    "philharvey/exiftool": "^12.",
    "vanderlee/syllable": "^1.",
    "ext-dom": "*",
    "ext-gd": "*",
    "ext-PDO": "*",
    "ext-exif": "*",
    "ext-sqlite3": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-enchant": "*",
    "ext-intl": "*",
    "ext-pcntl": "*",
    "ext-imagick": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "PhotoDatabase\\": "../scripts/php/class",
      "WebsiteTemplate\\": "vendor/speich/websitetemplate/scripts/php"
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/speich/WebsiteTemplate"
    },
    {
      "type": "vcs",
      "url": "https://github.com/mkardakov/ExifTool_PHP_Stayopen"
    },
    {
      "type": "package",
      "package": {
        "name": "philharvey/exiftool",
        "version": "12.63",
        "dist": {
          "url": "https://exiftool.org/Image-ExifTool-12.63.tar.gz",
          "type": "tar"
        }
      }
    }
  ],
  "config": {
    "platform": {
      "php": "8.2"
    },
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "name": "speich/photodb",
  "description": "Database to manage photos",
  "license": [
    "CC-BY-NC-SA-4.0"
  ]
}
